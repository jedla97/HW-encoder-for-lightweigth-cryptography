u8 key(u8 *key, unsigned int key1, unsigned int key2, unsigned int key3) {
	key[0] = (first_four_bytes >> 24) & 0xff;
	key[1] = (first_four_bytes >> 16) & 0xff;
	key[2] = (first_four_bytes >> 8) & 0xff;
	key[3] = (first_four_bytes) & 0xff;
	key[4] = (first_four_bytes >> 24) & 0xff;
	key[5] = (first_four_bytes >> 16) & 0xff;
	key[6] = (first_four_bytes >> 8) & 0xff;
	key[7] = (first_four_bytes) & 0xff;
	key[8] = (first_four_bytes >> 24) & 0xff;
	key[9] = (first_four_bytes >> 16) & 0xff;
